  body(itemscope="" itemtype="https://schema.org/WebPage")
    +link({
      class: "skip-main",
      href: "#content",
      content: "Skip to main content"
    })

    block navbar
      +navbar(page)

    main.main.transition-in-view(data-container tabindex="-1")
      if false 
        block hero
        section(id="content" itemtype="http://schema.org/WebPageElement" itemscope itemprop="mainEntity")
          block content
      else
        section(id="content" itemtype="http://schema.org/WebPageElement" itemscope itemprop="mainEntity")
          //- Intro layer
          +layer
            +layout({
              class: "layout-contain layer-overflow-hide"
            })
              section(class="layout-shorten layout-max-height-limited layout-flex layout-align-items-center layout-justify-content-center")
                section.style-center.layout-margin-top-larger--device-phone
                  +head({
                    class: "layout-margin-bottom"
                  })
                    h1.style-font-45--device-phone.style-primary-dark.style-bold.style-uppercase.layout-margin-dull Coming Soon.
                  +main
                    +p("Sorry, the page you are looking for is currently under maintenence.")
                    div
                      span(itemscope itemtype="https://schema.org/SiteNavigationElement")
                      +link({
                        href: "https://blog.okikio.dev",
                        class: "layout-margin-left-mini",
                        content: "Blog"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Blog link.")
                      span= " - "
                      +link({
                        href: "https://www.linkedin.com/in/okikio",
                        class: "layout-margin-left-mini",
                        content: "LinkedIn"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo LinkedIn page.")
                      span= " - "
                      +link({
                        href: "https://github.com/okikio",
                        class: "layout-margin-left-mini",
                        content: "Github"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Github link.")
                      span= " - "
                      +link({
                        href: "https://twitter.com/okikio_dev",
                        class: "layout-margin-left-mini",
                        content: "Twitter"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Twitter link.")
                      span= " - "
                      +link({
                        href: "https://codepen.io/okikio",
                        class: "layout-margin-left-mini",
                        content: "Codepen"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Codepen link.")
                      span= " - "
                      +link({
                        href: "https://dev.to/okikio",
                        class: "layout-margin-left-mini",
                        content: "Dev.to"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Dev.to link.")
                      span= " - "
                      +link({
                        href: "https://stackoverflow.com/users/12140185/okikio?tab=profile",
                        class: "layout-margin-left-mini",
                        content: "Stackoverflow"
                      })(target="_blank" rel="noopener" title="The official Okiki Ojo Stackoverflow link.")
                      span= " - "
                      span(itemprop="publisher" itemscope itemtype="https://schema.org/Organization" itemid="https://okikio.dev")
                        +link({
                          href: "mailto:hey@okikio.dev",
                          class: "layout-margin-left-mini",
                          content: "Email"
                        })(target="_blank" rel="noopener" itemprop="email" title="An email you can contact me with.")

    .layer-action-center.layer-action-center-hide
      article.layer-action-group
        .layer-round.layout-align-self-end.layout-margin-horz-small
          button.back-to-top.action-btn.action-btn-hover.layer-round.layer-primary.layer-primary-light-hover.layer-primary-light-focus.style-black.style-hover-black.style-focus-black(type="button")
            section.action-btn-layout.layout-padding-smallest
              span.action-btn-group
                span.action-btn-section
                  span.action-btn-spacer
                  span.action-btn-spacer
                  span.action-btn-content Back To Top
                i.action-icon.style-font-size-24(inline) expand_less

    block footer
      +footer({
        class: "footer-stuck layout-margin-vert-top"
      })

    script(src="/js/vendor.min.js" ph-inline)
